    <script type="text/javascript">
        if (lang == 'EN') {
            // Fix cb_options for EN error page
            cb_options.storage_path = cb_options.storage_path + 'en/';

            var urlErrorPage = '/wp-content/storage/en/error/index.html';

            jQuery.ajax({
                url: urlErrorPage,
                type: 'GET',
                success: function(data) {
                    var bodyError = data
                        .split(/<!-- CONTENT_START_ERROR -->/)[1]
                        .split(/<!-- CONTENT_END_ERROR -->/)[0];
                    jQuery("body").html(bodyError);
                    dbg.log('Se ha sustituido el contenido en inglés');

                },
                error: function() {
                    console.log('Se ha producido un error de conexión');
                }
            });

        }
    </script>
