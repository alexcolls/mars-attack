                        jQuery(document).ready(function($) {
                            var adminBar = $("#wpadminbar");
                            var t = null;

                            adminBar.mouseenter(function() {
                                if(t) {
                                    clearTimeout(t);
                                    t = null;
                                }

                                adminBar.addClass("visible");
                            });

                            adminBar.mouseleave(function() {
                                if(t) {
                                    clearTimeout(t);
                                    t = null;
                                }

                                t = setTimeout(function() {
                                    adminBar.removeClass("visible");
                                }, 1000);

                            });
                        });
                    </script>
