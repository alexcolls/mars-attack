<div class="paragraph paragraph--type--push-blocs">
          <h2 class="title">Nos derniers résultats financiers</h2>
    <div class="item-wrapper">
      
<div class="paragraph item paragraph--type--push-blocs-item">
        


                    <div class="media media--bundle--image media--blazy media--loading media--responsive media--image">
      <noscript>    <picture>
                  <source srcset="/sites/default/files/styles/push_blocs_desktop/public/image/2022-05/20220505_Resultats_Financiers_692x411.jpg?h=cb93dabd&amp;itok=wcC8ZI4k 1x" media="screen and (min-width: 90rem)" type="image/jpeg"/>
              <source srcset="/sites/default/files/styles/push_blocs_desktop/public/image/2022-05/20220505_Resultats_Financiers_692x411.jpg?h=cb93dabd&amp;itok=wcC8ZI4k 1x" media="screen and (min-width: 64rem)" type="image/jpeg"/>
              <source srcset="/sites/default/files/styles/push_blocs_desktop/public/image/2022-05/20220505_Resultats_Financiers_692x411.jpg?h=cb93dabd&amp;itok=wcC8ZI4k 1x" media="screen and (min-width: 48rem)" type="image/jpeg"/>
              <source srcset="/sites/default/files/styles/push_blocs_mobile/public/image/2022-05/20220505_Resultats_Financiers_692x411.jpg?h=cb93dabd&amp;itok=xfY_5ZMP 1x" type="image/jpeg"/>
                    <img class="media__image media__element" loading="lazy" src="/sites/default/files/styles/push_blocs_mobile/public/image/2022-05/20220505_Resultats_Financiers_692x411.jpg?h=cb93dabd&amp;itok=xfY_5ZMP" typeof="foaf:Image" alt=""/>

